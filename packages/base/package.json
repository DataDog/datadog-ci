{
  "name": "@datadog/datadog-ci-base",
  "version": "3.20.0",
  "license": "Apache-2.0",
  "homepage": "https://github.com/DataDog/datadog-ci/tree/master/packages/base",
  "repository": {
    "type": "git",
    "url": "https://github.com/DataDog/datadog-ci.git",
    "directory": "packages/base"
  },
  "exports": {
    "./cli": {
      "development": "./src/cli.ts",
      "default": "./dist/cli.js"
    },
    "./commands/*": {
      "development": "./src/commands/*.ts",
      "default": "./dist/commands/*.js"
    },
    "./constants": {
      "development": "./src/constants.ts",
      "default": "./dist/constants.js"
    },
    "./helpers/*": {
      "development": "./src/helpers/*.ts",
      "default": "./dist/helpers/*.js"
    },
    "./version": {
      "development": "./src/version.ts",
      "default": "./dist/version.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "yarn package:clean; yarn package:build",
    "lint": "yarn package:lint",
    "prepack": "yarn package:clean-dist"
  },
  "peerDependencies": {
    "@datadog/datadog-ci-plugin-synthetics": "workspace:*"
  },
  "dependencies": {
    "@types/datadog-metrics": "0.6.1",
    "async-retry": "1.3.1",
    "axios": "^1.11.0",
    "chalk": "3.0.0",
    "clipanion": "^3.2.1",
    "datadog-metrics": "0.9.3",
    "deep-extend": "0.6.0",
    "fast-xml-parser": "^4.4.1",
    "form-data": "^4.0.4",
    "glob": "^10.4.5",
    "inquirer": "^8.2.5",
    "jszip": "^3.10.1",
    "proxy-agent": "^6.4.0",
    "semver": "^7.5.3",
    "simple-git": "3.16.0",
    "terminal-link": "2.1.1",
    "tiny-async-pool": "^2.1.0",
    "typanion": "^3.14.0",
    "upath": "^2.0.1"
  },
  "devDependencies": {
    "@types/async-retry": "1.4.8",
    "@types/deep-extend": "0.4.31",
    "@types/inquirer": "8.2.6",
    "@types/semver": "^7.7.1",
    "@types/tiny-async-pool": "^2.0.3",
    "proxy": "^2.1.1"
  }
}
