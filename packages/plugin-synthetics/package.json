{
  "name": "@datadog/datadog-ci-plugin-synthetics",
  "version": "4.0.3",
  "license": "Apache-2.0",
  "description": "Datadog CI plugin for `synthetics` commands",
  "keywords": [
    "datadog",
    "datadog-ci",
    "plugin"
  ],
  "homepage": "https://github.com/DataDog/datadog-ci/tree/master/packages/plugin-synthetics",
  "repository": {
    "type": "git",
    "url": "https://github.com/DataDog/datadog-ci.git",
    "directory": "packages/plugin-synthetics"
  },
  "main": "dist/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./commands/*": {
      "development": "./src/commands/*.ts",
      "default": "./dist/commands/*.js"
    }
  },
  "files": [
    "dist/**/*",
    "README",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn package:clean; yarn package:build",
    "lint": "yarn package:lint",
    "prepack": "yarn package:clean-dist"
  },
  "peerDependencies": {
    "@datadog/datadog-ci-base": "workspace:*"
  },
  "dependencies": {
    "axios": "^1.12.1",
    "chalk": "3.0.0",
    "debug": "^4.4.1",
    "deep-extend": "0.6.0",
    "fast-levenshtein": "^3.0.0",
    "get-value": "^4.0.1",
    "ora": "5.4.1",
    "set-value": "^4.1.0",
    "ssh2": "^1.16.0",
    "sshpk": "1.16.1",
    "upath": "^2.0.1",
    "ws": "^7.5.10",
    "xml2js": "0.5.0",
    "yamux-js": "0.1.2"
  },
  "devDependencies": {
    "@types/deep-extend": "0.4.31",
    "@types/set-value": "^4.0.3",
    "@types/ssh2": "1.15.5",
    "@types/sshpk": "1.10.5",
    "@types/ws": "7.2.9",
    "@types/xml2js": "0.4.9"
  }
}
